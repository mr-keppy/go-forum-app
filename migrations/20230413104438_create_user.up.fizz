create_table("users") {
  t.Column("id", "serial", {primary:true})
  t.Column("first_name", "string", {"size": 255})
  t.Column("last_name", "string", {"size": 255})
  t.Column("email", "string", {"size": 255})
  t.Column("password", "string", {"size": 255})
  t.Column("access_level", "integer", {})
}